{
    "version": "0.2.0",
    "configurations": [
      
        {
            "type": "node",
            "request": "launch",
            "name": "run server",
            "program": "${workspaceRoot}/startup.js"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Chrome?p=9000",
            "url": "http://localhost:9000",
            "webRoot": "${workspaceRoot}"
            
        }
    ]
}